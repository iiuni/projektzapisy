{
  "compilerOptions": {
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "sourceMap": true,
    "target": "esnext",
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strict": true,
    "jsx": "react",
    "module": "esnext",
    "baseUrl": "./common/assets",
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "types": ["jquery", "lodash", "bootstrap", "popper.js"]
  },
  "exclude": ["./node_modules", "./compiled_assets"]
}
