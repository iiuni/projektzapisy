{
    "presets": [
        ["env", {
          "targets": {
            "browsers": ["last 2 versions", "ie >= 10"]
          },
          "modules": false
        }]
    ],
    "plugins": [
      "transform-runtime",
      ["transform-imports", {
        "lodash": {
          "transform": "lodash/${member}",
          "preventFullImport": true
        }
      }]
    ]
}
